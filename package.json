{
  "name": "boilerplate-react-auth",
  "version": "1.0.0",
  "description": "boilerplate for client react apps using authO",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "distDir": "./public"
    }
  },
  "scripts": {
    "start": "rm -rf dev && parcel --open --port 3030 --dist-dir dev",
    "build": "parcel build",
    "clear": "rm -rf public && rm -rf dev"
  },
  "keywords": [
    "parcel"
  ],
  "author": "dan moneypenny",
  "license": "MIT",
  "devDependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "parcel": "^2.2.1"
  },
  "dependencies": {
    "dotenv": "^14.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  }
}
